@model WebService.Models.Reservation

<div class="panel-heading">
    Create Reservation
</div>
<div class="panel-body">
    @using (Html.BeginForm("Add", "Home"))
    {
        <div class="form-group">
            <label>@Html.DisplayNameFor(t => t.ClientName)</label>
            @Html.TextBoxFor(t => t.ClientName, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label>@Html.DisplayNameFor(t => t.Location)</label>
            @Html.TextBoxFor(t => t.Location, new { @class = "form-control" })
        </div>
        <button type="submit" class="btn btn-primary">Save</button>
    }
</div>
